<%= simple_form_for [@starship, @booking] do |f| %>
  <div class="form-inputs">
    <%= f.input :booking_start_date %>
    <%= f.input :booking_end_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, { class: " button btn btn-light", value: "Create Booking" } %>
  </div>
<% end %>